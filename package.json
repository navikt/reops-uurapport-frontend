{
  "name": "reops-uurapport-frontend",
  "version": "0.0.1",
  "scripts": {
    "mock-dev": "concurrently \"pnpm run mock\" \"pnpm run dev:mock\"",
    "dev": "APP_ENV=local VITE_APP_ENV=local next dev",
    "dev:mock": "APP_ENV=mock VITE_APP_ENV=mock next dev",
    "prettier": "prettier --write .",
    "start": "next start",
    "build": "next build",
    "mock": "tsx mock/server.ts",
    "lint": "next lint",
    "knip": "knip",
    "knip:production": "knip --production",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.7",
    "@navikt/aksel-icons": "8.1.0",
    "@navikt/ds-css": "8.1.0",
    "@navikt/ds-react": "8.1.0",
    "@navikt/oasis": "4.1.1",
    "lodash": "^4.17.23",
    "next": "^15.5.11",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@hono/node-server": "1.19.9",
    "@types/lodash": "^4.17.23",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "^15.1.6",
    "hono": "4.11.7",
    "husky": "^9.1.7",
    "knip": "^5.83.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "lodash": "^4.17.23"
    }
  }
}
