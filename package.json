{
  "name": "reops-uurapport-frontend",
  "version": "0.0.1",
  "scripts": {
    "mock-dev": "concurrently \"pnpm run mock\" \"pnpm run dev:mock\"",
    "dev": "APP_ENV=local VITE_APP_ENV=local next dev",
    "dev:mock": "APP_ENV=mock VITE_APP_ENV=mock next dev",
    "prettier": "prettier --write .",
    "start": "next start",
    "build": "next build",
    "mock": "tsx mock/server.ts",
    "lint": "eslint .",
    "knip": "knip",
    "knip:production": "knip --production",
    "prepare": "husky",
    "analyze": "ANALYZE=true next build -c next.config.analyze.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.7",
    "@navikt/aksel-icons": "8.1.0",
    "@navikt/ds-css": "8.1.0",
    "@navikt/ds-react": "8.1.0",
    "@navikt/oasis": "4.1.1",
    "lodash": "^4.17.23",
    "next": "^16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "recharts": "^3.7.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@hono/node-server": "1.19.9",
    "@next/bundle-analyzer": "^16.1.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/lodash": "^4.17.23",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "hono": "4.11.7",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "knip": "^5.83.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "lodash": "^4.17.23"
    }
  }
}
