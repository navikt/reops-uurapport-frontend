{
  "name": "reops-uurapport-frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "mock-dev": "concurrently \"npm run mock\" \"npm run dev:mock\"",
    "dev": "APP_ENV=local VITE_APP_ENV=local astro dev",
    "dev:mock": "APP_ENV=mock VITE_APP_ENV=mock astro dev",
    "prettier": "prettier --write .",
    "start": "astro dev",
    "build": "astro check && astro build",
    "mock": "tsx mock/server.ts",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/node": "9.5.2",
    "@astrojs/react": "4.4.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.7",
    "@mui/x-charts": "^8.26.0",
    "@navikt/aksel-icons": "8.1.0",
    "@navikt/ds-react": "8.1.0",
    "@navikt/oasis": "4.1.1",
    "astro": "5.16.14",
    "lodash": "^4.17.23",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@hono/node-server": "1.19.9",
    "@navikt/ds-tokens": "^8.1.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "hono": "4.11.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  }
}
